{
  "scripts": {
    "client": "npm run start --prefix client",
    "server": "npm run start --prefix server",
    "clientinstall": "npm i --legacy-peer-deps --prefix client",
    "serverinstall": "npm i --legacy-peer-deps --prefix server",
    "start": "concurrently \"npm run clientinstall\" \"npm run serverinstall\" \"npm run client\" \"npm run server\" "
  },
  "dependencies": {
    "concurrently": "^8.2.0"
  }
}
